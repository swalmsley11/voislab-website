{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VoisLabS3Access",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::voislab-*",
        "arn:aws:s3:::*-voislab-*",
        "arn:aws:s3:::voislab-*/*",
        "arn:aws:s3:::*-voislab-*/*"
      ]
    },
    {
      "Sid": "VoisLabDynamoDBAccess",
      "Effect": "Allow",
      "Action": "dynamodb:*",
      "Resource": [
        "arn:aws:dynamodb:*:*:table/voislab-*",
        "arn:aws:dynamodb:*:*:table/*-voislab-*",
        "arn:aws:dynamodb:*:*:table/voislab-*/index/*",
        "arn:aws:dynamodb:*:*:table/*-voislab-*/index/*"
      ]
    },
    {
      "Sid": "VoisLabLambdaAccess",
      "Effect": "Allow",
      "Action": "lambda:*",
      "Resource": [
        "arn:aws:lambda:*:*:function:voislab-*",
        "arn:aws:lambda:*:*:function:*-voislab-*",
        "arn:aws:lambda:*:*:layer:voislab-*",
        "arn:aws:lambda:*:*:layer:*-voislab-*"
      ]
    },
    {
      "Sid": "VoisLabIAMAccess",
      "Effect": "Allow",
      "Action": [
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:GetRole",
        "iam:UpdateRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:PutRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:GetRolePolicy",
        "iam:ListRolePolicies",
        "iam:ListAttachedRolePolicies",
        "iam:PassRole",
        "iam:TagRole",
        "iam:UntagRole",
        "iam:ListRoleTags"
      ],
      "Resource": [
        "arn:aws:iam::*:role/voislab-*",
        "arn:aws:iam::*:role/*-voislab-*"
      ]
    },
    {
      "Sid": "VoisLabSSMAccess",
      "Effect": "Allow",
      "Action": "ssm:*",
      "Resource": [
        "arn:aws:ssm:*:*:parameter/voislab/*",
        "arn:aws:ssm:*:*:parameter/voislab-*"
      ]
    },
    {
      "Sid": "GlobalAWSServices",
      "Effect": "Allow",
      "Action": [
        "cloudfront:*",
        "amplify:*",
        "route53:*",
        "acm:*",
        "cloudformation:*",
        "logs:*",
        "cloudwatch:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "CDKOperationsAccess",
      "Effect": "Allow",
      "Action": [
        "sts:AssumeRole"
      ],
      "Resource": "arn:aws:iam::*:role/cdk-*"
    },
    {
      "Sid": "CDKAssetsAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cdk-*",
        "arn:aws:s3:::cdk-*/*"
      ]
    }
  ]
}