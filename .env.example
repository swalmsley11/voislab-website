# VoisLab Website Environment Configuration

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_secret_key_here

# DynamoDB Configuration
VITE_DYNAMODB_TABLE_NAME=voislab-tracks-dev

# S3 Configuration
VITE_S3_MEDIA_BUCKET=voislab-media-dev
VITE_S3_UPLOAD_BUCKET=voislab-upload-dev

# CloudFront Configuration
VITE_CLOUDFRONT_DOMAIN=your-cloudfront-domain.cloudfront.net

# Google Analytics Configuration
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Environment
VITE_ENVIRONMENT=dev

# Monitoring and Analytics Configuration
VITE_ERROR_REPORTING_ENABLED=true
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_MONITORING_SAMPLE_RATE=1.0
VITE_MAX_ERRORS_PER_SESSION=10

# Feature Flags
VITE_ANALYTICS_ENABLED=true
VITE_DEBUG_MODE=false